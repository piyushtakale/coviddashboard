<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>  
    <servlet-name>welcomer</servlet-name>  
    <jsp-file>/welcome.jsp</jsp-file>  
  
    <init-param>  
      <param-name>dname</param-name>  
      <param-value>sun.jdbc.odbc.JdbcOdbcDriver</param-value>  
    </init-param>  
  
  </servlet>

  <servlet>
  	<servlet-name>Test</servlet-name>
  	<display-name>Test</display-name>
  	<description></description>
  	<servlet-class>piyush.Test</servlet-class>
  </servlet>

  <servlet>
  	<servlet-name>cdata</servlet-name>
  	<display-name>cdata</display-name>
  	<description></description>
  	<servlet-class>display.cdata</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>states</servlet-name>
  	<display-name>states</display-name>
  	<description></description>
  	<servlet-class>display.StateData</servlet-class>
  </servlet>
 <servlet> 
  	<servlet-name>nation</servlet-name>
  	<display-name>nation</display-name>
  	<description></description>
  	<servlet-class>display.NationData</servlet-class>
  </servlet>
  <servlet> 
  	<servlet-name>stateall</servlet-name>
  	<display-name>stateall</display-name>
  	<description></description>
  	<servlet-class>dataservice.StateAll</servlet-class>
  </servlet>
  <servlet> 
  	<servlet-name>stateone</servlet-name>
  	<display-name>stateone</display-name>
  	<description></description>
  	<servlet-class>dataservice.StateOne</servlet-class>
  </servlet>
  <servlet> 
  	<servlet-name>statelist</servlet-name>
  	<display-name>statelist</display-name>
  	<description></description>
  	<servlet-class>dataservice.StateList</servlet-class>
  </servlet>
  <servlet> 
  	<servlet-name>districtlist</servlet-name>
  	<display-name>districtlist</display-name>
  	<description></description>
  	<servlet-class>dataservice.DistrictList</servlet-class>
  </servlet>
  <servlet> 
  	<servlet-name>districtone</servlet-name>
  	<display-name>districtone</display-name>
  	<description></description>
  	<servlet-class>dataservice.DistrictOne</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  	<servlet-name>Test</servlet-name>
  	<url-pattern>/Test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>  
    <servlet-name>welcomer</servlet-name>  
    <url-pattern>/welcome</url-pattern>  
  </servlet-mapping>  

  <servlet-mapping>  
    <servlet-name>cdata</servlet-name>  
    <url-pattern>/cdata</url-pattern>  
  </servlet-mapping>  

  <servlet-mapping>  
    <servlet-name>states</servlet-name>  
    <url-pattern>/states</url-pattern>  
  </servlet-mapping>  
  <servlet-mapping>  
    <servlet-name>nation</servlet-name>  
    <url-pattern>/nation</url-pattern>  
  </servlet-mapping>
  <servlet-mapping>  
    <servlet-name>stateall</servlet-name>  
    <url-pattern>/state/all</url-pattern>  
  </servlet-mapping>  
  <servlet-mapping>  
    <servlet-name>stateone</servlet-name>  
    <url-pattern>/state/one</url-pattern>  
  </servlet-mapping>  
  <servlet-mapping>  
    <servlet-name>statelist</servlet-name>  
    <url-pattern>/state/list</url-pattern>  
  </servlet-mapping> 
  <servlet-mapping>  
    <servlet-name>districtlist</servlet-name>  
    <url-pattern>/district/list</url-pattern>  
  </servlet-mapping> 
  <servlet-mapping>  
    <servlet-name>districtone</servlet-name>  
    <url-pattern>/district/one</url-pattern>  
  </servlet-mapping> 
</web-app>
